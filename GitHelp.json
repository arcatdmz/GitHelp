    {
      "title": "ファイルリスト",
      "created": 1520137762,
      "updated": 1520215308,
      "lines": [
        {
          "text": "ファイルリスト",
          "created": 1520137762,
          "updated": 1520137762
        },
        {
          "text": "引数の中にファイル名とマッチするものがあればファイルリストを取得         ",
          "created": 1520137793,
          "updated": 1520137802
        },
        {
          "text": "",
          "created": 1520215285,
          "updated": 1520215285
        },
        {
          "text": "code:files.rb",
          "created": 1520137802,
          "updated": 1520215289
        },
        {
          "text": " def files(argv=ARGV)",
          "created": 1520215289,
          "updated": 1520215295
        },
        {
          "text": "   list = begin",
          "created": 1520215295,
          "updated": 1520215295
        },
        {
          "text": "            `git ls-files`.split(/\\n/)",
          "created": 1520215295,
          "updated": 1520215295
        },
        {
          "text": "          rescue",
          "created": 1520215295,
          "updated": 1520215295
        },
        {
          "text": "            []",
          "created": 1520215295,
          "updated": 1520215295
        },
        {
          "text": "          end",
          "created": 1520215295,
          "updated": 1520215295
        },
        {
          "text": "   matched = {}",
          "created": 1520215295,
          "updated": 1520215295
        },
        {
          "text": "   list.each { |file|",
          "created": 1520215295,
          "updated": 1520215295
        },
        {
          "text": "     if argv.length == 0",
          "created": 1520215295,
          "updated": 1520215295
        },
        {
          "text": "       matched[file] = true",
          "created": 1520215295,
          "updated": 1520215295
        },
        {
          "text": "     else",
          "created": 1520215295,
          "updated": 1520215295
        },
        {
          "text": "       args(argv).each { |arg|",
          "created": 1520215295,
          "updated": 1520215295
        },
        {
          "text": "         matched[file] = true if file.match arg",
          "created": 1520215295,
          "updated": 1520215295
        },
        {
          "text": "       }",
          "created": 1520215295,
          "updated": 1520215295
        },
        {
          "text": "     end",
          "created": 1520215295,
          "updated": 1520215295
        },
        {
          "text": "   }",
          "created": 1520215295,
          "updated": 1520215295
        },
        {
          "text": "   files = list",
          "created": 1520215295,
          "updated": 1520215295
        },
        {
          "text": "   files = matched.keys if matched.keys.length > 0",
          "created": 1520215295,
          "updated": 1520215295
        },
        {
          "text": "   files.join(\"|\")",
          "created": 1520215295,
          "updated": 1520215295
        },
        {
          "text": " end",
          "created": 1520215295,
          "updated": 1520215295
        },
        {
          "text": "",
          "created": 1520215305,
          "updated": 1520215305
        },
        {
          "text": "[引数パラメタ]",
          "created": 1520215305,
          "updated": 1520215305
        },
        {
          "text": "",
          "created": 1520142581,
          "updated": 1520142581
        }
      ]
    },
    {
      "title": "args",
      "created": 1520160117,
      "updated": 1520199968,
      "lines": [
        {
          "text": "args",
          "created": 1520160117,
          "updated": 1520160117
        },
        {
          "text": "code:args.rb",
          "created": 1520160117,
          "updated": 1520160120
        },
        {
          "text": " def args(argv=ARGV)",
          "created": 1520199890,
          "updated": 1520199890
        },
        {
          "text": "   a = []",
          "created": 1520199890,
          "updated": 1520199890
        },
        {
          "text": "   argv.each { |arg|",
          "created": 1520199890,
          "updated": 1520199890
        },
        {
          "text": "     if arg =~ /^(\\d+)([^\\d]+)?$/ then",
          "created": 1520199890,
          "updated": 1520199890
        },
        {
          "text": "       a << $1",
          "created": 1520199890,
          "updated": 1520199890
        },
        {
          "text": "       a << $2 if $2.to_s != ''",
          "created": 1520199890,
          "updated": 1520199890
        },
        {
          "text": "     else",
          "created": 1520199890,
          "updated": 1520199890
        },
        {
          "text": "       a << arg.sub(/^['\"]/,'').sub(/['\"]$/,'').sub('「','').sub('」','')",
          "created": 1520199890,
          "updated": 1520199890
        },
        {
          "text": "     end",
          "created": 1520199890,
          "updated": 1520199890
        },
        {
          "text": "   }",
          "created": 1520199890,
          "updated": 1520199890
        },
        {
          "text": "   a.length > 0 ? a : ['']",
          "created": 1520199890,
          "updated": 1520199890
        },
        {
          "text": " end",
          "created": 1520199890,
          "updated": 1520199890
        },
        {
          "text": "",
          "created": 1520160118,
          "updated": 1520160118
        }
      ]
