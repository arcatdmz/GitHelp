    {
      "title": "引数パラメタ",
      "created": 1495152243,
      "updated": 1520227476,
      "lines": [
        {
          "text": "引数パラメタ",
          "created": 1495152243,
          "updated": 1520137533
        },
        {
          "text": " 引数で与えられたパラメタ",
          "created": 1520132961,
          "updated": 1520137469
        },
        {
          "text": "  githelp 'abc'",
          "created": 1520137537,
          "updated": 1520149202
        },
        {
          "text": " ', \" で囲む?",
          "created": 1520137470,
          "updated": 1520137479
        },
        {
          "text": "",
          "created": 1520132961,
          "updated": 1520132961
        },
        {
          "text": "code:params.rb",
          "created": 1495152243,
          "updated": 1519893906
        },
        {
          "text": " def params(argv=ARGV)",
          "created": 1520199776,
          "updated": 1520199777
        },
        {
          "text": "   a = []",
          "created": 1520199777,
          "updated": 1520199777
        },
        {
          "text": "   argv.each { |arg|",
          "created": 1520199777,
          "updated": 1520199777
        },
        {
          "text": "     if arg =~ /^(\\d+)[^\\d]*$/ then",
          "created": 1520199777,
          "updated": 1520199777
        },
        {
          "text": "       # a << $1                                                                                                                                                                             ",
          "created": 1520199777,
          "updated": 1520199777
        },
        {
          "text": "     elsif arg =~ /^'.*'$/ || arg =~ /^\".*\"$/ || arg =~ /^「.*」$/",
          "created": 1520199777,
          "updated": 1520199777
        },
        {
          "text": "       a << arg.sub(/^['\"]/,'').sub(/['\"]$/,'').sub('「','').sub('」','')",
          "created": 1520199777,
          "updated": 1520199777
        },
        {
          "text": "     else",
          "created": 1520227469,
          "updated": 1520227471
        },
        {
          "text": "       a << arg",
          "created": 1520227472,
          "updated": 1520227476
        },
        {
          "text": "     end",
          "created": 1520199777,
          "updated": 1520199777
        },
        {
          "text": "   }",
          "created": 1520199777,
          "updated": 1520199777
        },
        {
          "text": "   a.length > 0 ? a.join('|') : 'xxxxx'",
          "created": 1520199777,
          "updated": 1520199777
        },
        {
          "text": " end",
          "created": 1520199777,
          "updated": 1520199777
        },
        {
          "text": "",
          "created": 1520137414,
          "updated": 1520137414
        }
      ]
